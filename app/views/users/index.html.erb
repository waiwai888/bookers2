<div class='row'>
  <div class="col-md-3">
    <%= render 'books/list', user: current_user, book: @book %>
  </div>
  <div class="col-md-8 offset-md-1">
  <h1>Users</h1>
    <table>
      <thead>
        <tr>
          <th>image</th>
          <th>name</th>
        </tr>
      </thead>
      <tbody>
        <% @users.each do |user| %>
        <tr>
          <td><%= attachment_image_tag user, :profile_image, :fill, 100, 100 ,fallback: "noimage.jpeg"%></td>
          <td><%= user.name %></td>
          <td><%= link_to "Show", user_path(user.id) %></td>
        </tr>
        <% end %>
      </tbody>
    </table>
  </div>
</div>